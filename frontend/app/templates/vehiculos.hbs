

<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Jquery utilitary -->
    <script type="text/javacsript" src="jquery-3.6.0.min.js"></script>

    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

    <title>Vehiculos</title>
  </head>


  <body>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>

    <br/><br/>



    <div class="container">
        <div class="row">
            <div class="col-1"></div>
            <div class="col-8"></div>
            <div class="col-2">
                <a href="http://localhost:8080/index.php/gilasw/autenticacion/login">
                    <button class="btn btn-link btn-block">Cerrar sesión</button>
                </a>
                
            </div>
            <div class="col-1"></div>
        </div>
    </div>

    <center><h1>Catalogo vehiculos</h1></center>

    <div class="container">
        <div class="row">

            <div class="col-1"></div>
            
            <div class="col-8"></div>
            
            <div class="col-2 text-right">
                <AgregarVehiculoComponent/>
            </div>

            <div class="col-1"></div>
            
        </div>
    </div>
    
    <br/>

    <div class="container">
        <div class="row">

            <div class="col-1"></div>

            <div class="col-10">
                
                <table class="table table-striped">
                    <thead>
                        <tr>
                        <th scope="col">ID</th>
                        <th scope="col">Tipo vehículo</th>
                        <th scope="col">Número de llantas</th>
                        <th scope="col">Potencia (HP)</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#each this.vehiculosData as |vehiculo|}}
                            <tr>
                                <th scope="row">{{vehiculo.vehiculo_k}}</th>
                                    <td>{{vehiculo.tipo_vehiculo}}</td>
                                    <td>{{vehiculo.numero_llantas}}</td>
                                    <td>{{vehiculo.potencia_motor}}</td>
                            </tr>
                        {{else}}
                        {{/each}}
                    </tbody>
                </table>

            </div>

            <div class="col-1"></div>
            
            
        </div>
    </div>



  </body>
</html>

<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">


{{outlet}}